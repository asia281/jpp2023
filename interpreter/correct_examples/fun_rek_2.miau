fun pow2(int exp) -> int {
    if (exp == 0) {
        return 1;
    }
    return 2 * pow2(exp - 1);
}

int pow = pow2(5);

printEndl("Expected: 32, got:", pow);

fun factorial(int exp) -> int {
    if (exp == 1) {
        return 1;
    }
    return exp * factorial(exp - 1);
}

int fac = factorial(6);

printEndl("Expected: 720, got:", fac);